{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-multiple-empty-lines": ["error", { "max": 2 }],
    "no-console": 0,
    "no-unreachable": 0,
    "no-unused-vars": 0,
    "no-extra-semi": 1,
  }
}